<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='CampScribe'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />
	<inherits name="com.google.gwt.json.JSON" />

	<!-- Inherit the default GWT style sheet. You can change -->
	<!-- the theme of your GWT application by uncommenting -->
	<!-- any one of the following lines. -->
	<inherits name='com.google.gwt.user.theme.clean.Clean' />
	<!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/> -->

	<!-- Other module inherits -->

	<!-- Specify the app entry point class. -->
    <entry-point class='com.campscribe.client.CampInfoModule' />
    <entry-point class='com.campscribe.client.ClazzModule' />
    <entry-point class='com.campscribe.client.EventModule' />
	<entry-point class='com.campscribe.client.MeritBadgeMetadataModule' />
    <entry-point class='com.campscribe.client.MeritBadgeModule' />
	<entry-point class='com.campscribe.client.ScoutModule' />
    <entry-point class='com.campscribe.client.ScoutClazzModule' />
	<entry-point class='com.campscribe.client.StaffModule' />

	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />

</module>
