<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
        .important {
            font-weight: bold;
        }
        .wide {
            width: 100%;
        }
	</ui:style>
	<g:HTMLPanel>
		<table>
            <tr>
                <td>Unit:</td>
                <td>
                    <g:ListBox ui:field="unitList" allowMultiSelect="false" />
                </td>
            </tr>
		</table>
		<g:FlowPanel ui:field="searchResultsPanel" >
		    <g:ListBox ui:field="searchResults" allowMultiSelect="true" visibleItemCount="10" styleName="{style.wide}"/>
		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder> 